{
  "name": "admin-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,vue}'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@vue/cli": "^5.0.8",
    "ant-design-vue": "^4.2.3",
    "apexcharts": "^3.50.0",
    "axios": "^1.7.2",
    "chart.js": "^4.4.4",
    "core-js": "^3.8.3",
    "defaults": "^3.0.0",
    "echarts": "^5.5.1",
    "element-plus": "^2.7.7",
    "file-saver": "^2.0.5",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.3",
    "mitt": "^3.0.1",
    "plotly.js-dist": "^2.35.0",
    "vue": "^3.4.38",
    "vue-apexcharts": "^1.6.2",
    "vue-chartjs": "^5.3.1",
    "vue-cookies": "^1.8.4",
    "vue-i18n": "^9.13.1",
    "vue-resizable": "^2.1.7",
    "vue-router": "^4.4.0",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.6.13",
    "vuex": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "^5.0.8",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^8.0.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.0",
    "prettier": "^2.8.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "comma-dangle": [
        "error",
        "never"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
